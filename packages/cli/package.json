{
  "name": "@deep-agent/cli",
  "version": "1.0.0",
  "description": "Deep agent CLI - Command-line interface for multi-turn AI conversations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "deep": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@deep-agent/core": "*",
    "commander": "^12.1.0",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "inquirer": "^12.3.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7"
  },
  "engines": {
    "node": ">=18"
  }
}